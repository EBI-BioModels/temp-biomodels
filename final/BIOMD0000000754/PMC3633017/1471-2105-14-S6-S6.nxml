mml:mi>a</mml:mi><mml:mi>S</mml:mi></mml:mrow><mml:mo class="MathClass-close">)</mml:mo></mml:mrow></mml:mrow></mml:mfrac></mml:math></inline-formula></td><td align="left"><inline-formula><mml:math id="M44" name="1471-2105-14-S6-S6-i46" overflow="scroll"><mml:mfrac><mml:mrow><mml:mi>p</mml:mi><mml:mn>3</mml:mn><mml:mi>.</mml:mi><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>u</mml:mi><mml:mi>m</mml:mi><mml:mi>o</mml:mi><mml:mi>u</mml:mi><mml:mi>r</mml:mi></mml:mrow><mml:mrow><mml:mrow><mml:mo class="MathClass-open">(</mml:mo><mml:mrow><mml:mi>g</mml:mi><mml:mn>4</mml:mn><mml:mo class="MathClass-bin">+</mml:mo><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>u</mml:mi><mml:mi>m</mml:mi><mml:mi>o</mml:mi><mml:mi>u</mml:mi><mml:mi>r</mml:mi></mml:mrow><mml:mo class="MathClass-close">)</mml:mo></mml:mrow><mml:mrow><mml:mo class="MathClass-open">(</mml:mo><mml:mrow><mml:mn>1</mml:mn><mml:mo class="MathClass-bin">+</mml:mo><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>p</mml:mi><mml:mi>h</mml:mi><mml:mi>a</mml:mi><mml:mi>.</mml:mi><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>G</mml:mi><mml:mi>F</mml:mi></mml:mrow><mml:mo class="MathClass-close">)</mml:mo></mml:mrow></mml:mrow></mml:mfrac></mml:math></inline-formula></td></tr><tr><td/><td colspan="3"><hr/></td></tr><tr><td/><td align="left">KillTumour</td><td align="left"><inline-formula><mml:math id="M45" name="1471-2105-14-S6-S6-i47" overflow="scroll"><mml:mfrac><mml:mrow><mml:msub><mml:mrow><mml:mi>a</mml:mi></mml:mrow><mml:mrow><mml:mi>a</mml:mi></mml:mrow></mml:msub><mml:mi>.</mml:mi><mml:mi>T</mml:mi><mml:mi>E</mml:mi></mml:mrow><mml:mrow><mml:mi>g</mml:mi><mml:mn>2</mml:mn><mml:mo class="MathClass-bin">+</mml:mo><mml:mi>T</mml:mi></mml:mrow></mml:mfrac></mml:math></inline-formula></td><td align="left"><inline-formula><mml:math id="M46" name="1471-2105-14-S6-S6-i48" overflow="scroll"><mml:mfrac><mml:mrow><mml:mi>a</mml:mi><mml:mi>a</mml:mi><mml:mo class="MathClass-bin">&#x000d7;</mml:mo><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>u</mml:mi><mml:mi>m</mml:mi><mml:mi>o</mml:mi><mml:mi>u</mml:mi><mml:mi>r</mml:mi><mml:mo class="MathClass-bin">&#x000d7;</mml:mo><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>E</mml:mi><mml:mi>f</mml:mi><mml:mi>f</mml:mi><mml:mi>e</mml:mi><mml:mi>c</mml:mi><mml:mi>t</mml:mi><mml:mi>o</mml:mi><mml:mi>r</mml:mi></mml:mrow><mml:mrow><mml:mi>g</mml:mi><mml:mn>2</mml:mn><mml:mo class="MathClass-bin">+</mml:mo><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>u</mml:mi><mml:mi>m</mml:mi><mml:mi>o</mml:mi><mml:mi>u</mml:mi><mml:mi>r</mml:mi></mml:mrow></mml:mfrac></mml:math></inline-formula></td></tr><tr><td colspan="4"><hr/></td></tr><tr><td align="left">Tumour Cell</td><td align="left">Reproduce</td><td align="left"><inline-formula><mml:math id="M47" name="1471-2105-14-S6-S6-i49" overflow="scroll"><mml:mrow><mml:mo class="MathClass-open">(</mml:mo><mml:mrow><mml:mi>a</mml:mi><mml:mi>T</mml:mi><mml:mrow><mml:mo class="MathClass-open">(</mml:mo><mml:mrow><mml:mn>1</mml:mn><mml:mo class="MathClass-bin">-</mml:mo><mml:mfrac><mml:mrow><mml:mi>T</mml:mi></mml:mrow><mml:mrow><mml:mn>1000000000</mml:mn></mml:mrow></mml:mfrac></mml:mrow><mml:mo class="MathClass-close">)</mml:mo></mml:mrow></mml:mrow><mml:mo class="MathClass-close">)</mml:mo></mml:mrow></mml:math></inline-formula></td><td align="left"><inline-formula><mml:math id="M48" name="1471-2105-14-S6-S6-i50" overflow="scroll"><mml:mrow><mml:mo class="MathClass-open">(</mml:mo><mml:mrow><mml:mi>T</mml:mi><mml:mi>o</mml:mi><mml:mi>t</mml:mi><mml:mi>a</mml:mi><mml:mi>l</mml:mi><mml:mi>T</mml:mi><mml:mi>u</mml:mi><mml:mi>m</m