				\begin{document}$$\begin{aligned} \frac{dx_{\text {v}}}{dt}\,=\,&#x00026;H(t)+\delta _{i} b x_{\text {i}} -\omega x_{\text {v}} -d_{u}^{v}x_{\text {v}}\frac{x_{\text {e}}}{h_{e}+x_{\text {e}}} -d_{m1}^{v}x_{\text {v}}\frac{x_{\text {m1}}}{h_{m}+x_{\text {m2}}}, \end{aligned}$$\end{document}</tex-math><mml:math id="M66" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfrac><mml:mrow><mml:mi>d</mml:mi><mml:msub><mml:mi>x</mml:mi><mml:mtext>v</mml:mtext></mml:msub></mml:mrow><mml:mrow><mml:mi mathvariant="italic">dt</mml:mi></mml:mrow></mml:mfrac><mml:mspace width="0.166667em"/><mml:mo>=</mml:mo><mml:mspace width="0.166667em"/></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mi>H</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:msub><mml:mi>&#x003b4;</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mi>b</mml:mi><mml:msub><mml:mi>x</mml:mi><mml:mtext>i</mml:mtext></mml:msub><mml:mo>-</mml:mo><mml:mi>&#x003c9;</mml:mi><mml:msub><mml:mi>x</mml:mi><mml:mtext>v</mml:mtext></mml:msub><mml:mo>-</mml:mo><mml:msubsup><mml:mi>d</mml:mi><mml:mrow><mml:mi>u</mml:mi></mml:mrow><mml:mi>v</mml:mi></mml:msubsup><mml:msub><mml:mi>x</mml:mi><mml:mtext>v</mml:mtext></mml:msub><mml:mfrac><mml:msub><mml:mi>x</mml:mi><mml:mtext>e</mml:mtext></mml:msub><mml:mrow><mml:msub><mml:mi>h</mml:mi><mml:mi>e</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:msub><mml:mi>x</mml:mi><mml:mtext>e</mml:mtext></mml:msub></mml:mrow></mml:mfrac><mml:mo>-</mml:mo><mml:msubsup><mml:mi>d</mml:mi><mml:mrow><mml:mi>m</mml:mi><mml:mn>1</mml:mn></mml:mrow><mml:mi>v</mml:mi></mml:msubsup><mml:msub><mml:mi>x</mml:mi><mml:mtext>v</mml:mtext></mml:msub><mml:mfrac><mml:msub><mml:mi>x</mml:mi><mml:mtext>m1</mml:mtext></mml:msub><mml:mrow><mml:msub><mml:mi>h</mml:mi><mml:mi>m</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:msub><mml:mi>x</mml:mi><mml:mtext>m2</mml:mtext></mml:msub></mml:mrow>