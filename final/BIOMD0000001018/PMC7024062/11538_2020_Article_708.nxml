				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \dfrac{\mathrm{d}C3}{\mathrm{d}t}&#x00026;= k_{s1} - d_{1}C3 -k_{1}C3 - \dfrac{k_{2}\cdot C3bBb\cdot C3}{k_{3} + C3},\nonumber \\ \dfrac{\mathrm{d}C3b}{\mathrm{d}t}&#x00026;= k_{1}C3 + \dfrac{k_{2}\cdot C3bBb\cdot C3}{k_{3} + C3} - k_{4}C3b\cdot FB + k_{5}C3bB_c + k_{6}C3bBb,\nonumber \\ \dfrac{\mathrm{d}C3bB_c}{\mathrm{d}t}&#x00026;= k_{4}C3b\cdot FB - k_{5}C3bB_c - k_{9}C3bB_c + k_{10}C3bB_o,\nonumber \\ \dfrac{\mathrm{d}C3bB_o}{\mathrm{d}t}&#x00026;= k_{9}C3bB_c - k_{10}C3bB_o - \dfrac{k_{7}FD\cdot C3bB_o}{k_{8} + C3bB_o},\nonumber \\ \dfrac{\mathrm{d}C3bBb}{\mathrm{d}t}&#x00026;= \dfrac{k_{7}FD\cdot C3bB_o}{k_{8} + C3bB_o} - k_{6}C3bBb,\nonumber \\ \dfrac{\mathrm{d}FB}{\mathrm{d}t}&#x00026;= k_{s2} - d_{2}FB- k_{4}C3b\cdot FB + k_{5}C3bB_c. \end{aligned}$$\end{document}</tex-math><mml:math id="M490" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mstyle displaystyle="true" scriptlevel="0"><mml:mfrac><mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi>C</mml:mi><mml:mn>3</mml:mn></mml:mrow><mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi>t</mml:mi></mml:mrow></mml:mfrac></mml:mstyle></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>=</mml:mo><mml:msub><mml:mi>k</mml:mi><mml:mrow><mml:mi>s</mml:mi><mml:mn>1</mml:mn></mml:mrow></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mi>d</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mi>C</mml:mi><mml:mn>3</mml:mn><mml:mo>-</mml:mo><mml:msub><mml:mi>k</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mi>C</mml:mi><mml:mn>3</mml:mn><mml:mo>-</mml:mo><mml:mstyle displaystyle="true" scriptlevel="0"><mml:mfrac><mml:mrow><mml:msub><mml:mi>k</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:mo>&#x000b7;</mml:mo><mml:mi>C</mml:mi><mml:mn>3</mml:mn><mml:mi>b</mml:mi><mml:mi>B</mml:mi><mml:mi>b</mml:mi><mml:mo>&#x000b7;</mml:mo><mml:mi>C</mml:mi><mml:mn>3</mml:mn></mml:mrow><mml:mrow><mml:msub><mml:mi>k</mml:mi><mml:mn>3</mml:mn></mml:msub><mml:mo>+</mml:mo><mml:mi>C</