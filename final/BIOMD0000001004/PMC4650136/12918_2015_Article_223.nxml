				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document} $$\begin{array}{*{20}l} \frac{d [\!\text{ROR\(\gamma\)t}_{\text{mRNA}}] }{d t} = &#x00026;\quad \theta_{10} [\!\text{TGF\(\beta\)}_{\text{int}}] [\!\text{STAT3}_{\text{prot}^{*}}] \end{array} $$ \end{document}</tex-math><mml:math id="M32"><mml:mtable class="align" columnalign="left"><mml:mtr><mml:mtd class="align-1"><mml:mfrac><mml:mrow><mml:mi>d</mml:mi><mml:mo>[</mml:mo><mml:mspace width="0.3em"/><mml:msub><mml:mrow><mml:mtext>ROR</mml:mtext><mml:mspace width="1em"/><mml:mi>&#x003b3;</mml:mi><mml:mtext>t</mml:mtext></mml:mrow><mml:mrow><mml:mtext>mRNA</mml:mtext></mml:mrow></mml:msub><mml:mo>]</mml:mo></mml:mrow><mml:mrow><mml:mtext mathvariant="italic">dt</mml:mtext></mml:mrow></mml:mfrac><mml:mo>=</mml:mo></mml:mtd><mml:mtd class="align-2"><mml:mspace width="1em"/><mml:msub><mml:mrow><mml:mi>&#x003b8;</mml:mi></mml:mrow><mml:mrow><mml:mn>10</mml:mn></mml:mrow></mml:msub><mml:mo>[</mml:mo><mml:mspace width="0.3em"/><mml:msub><mml:mrow><mml:mtext>TGF</mml:mtext><mml:mspace width="1em"/><mml:mi>&#x003b2;</mml:mi><mml:mtext/></mml:mrow><mml:mrow><mml:mtext>int</mml:mtext></mml:mrow></mml:msub><mml:mo>]</mml:mo><mml:mo>[</mml:mo><mml:mspace width="0.3em"/><mml:msub><mml:mrow><mml:mtext>STAT3</mml:mtext></mml:mrow><mml:mrow><mml:msup><mml:mrow><mml:mtext>prot</mml:mtext></mml:mrow><mml:mrow><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup></mml:mrow></mml:msub><mml:mo>]</mml:mo><mml:mspace width="2em"/></mml:mtd></mml:mtr></mml:mtable></mml:math><graphic xlink:href="12918_2015_223_Article_Equ15.gif" position="anchor"/></alternatives></disp-formula></p><p><disp-formula id="Equ16"><label>(16)</label><alternatives><tex-math id="M33">\documentclass[12pt]{minimal}
