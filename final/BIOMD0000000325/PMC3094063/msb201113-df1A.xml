<?xml version="1.0" encoding="UTF-8"?><sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">  <model name="Minimal two-feedback model">    <notes>      <html xmlns="http://www.w3.org/1999/xhtml">        <body>          <p>Definitions for the model in Figure 1A           </p>          <p>L- Ligand                                        </p>          <p>R- Receptor                                      </p>          <p>C - Ligand-Receptor Complex                      </p>          <p>I - Inactive Transcription Factor                </p>          <p>X - C bound to I                                 </p>          <p>A - Active Transcription Factor</p>        </body>      </html>    </notes>    <listOfParameters>      <parameter id="L" name="L" value="0.1"/>      <parameter id="BR" name="BR" value="0.005"/>      <parameter id="kdegR" name="kdegR" value="0.005"/>      <parameter id="kon" name="kon" value="0.001"/>      <parameter id="koff" name="koff" value="0.05"/>      <parameter id="Rs" name="Rs" value="3"/>      <parameter id="KD" name="KD" value="200"/>      <parameter id="kdegC" name="kdegC" value="0.01"/>      <parameter id="k1" name="k1" value="1"/>      <parameter id="k2" name="k2" value="5"/>      <parameter id="k3" name="k3" value="45"/>      <parameter id="BI" name="BI" value="0.005"/>      <parameter id="kdegI" name="kdegI" value="0.005"/>      <parameter id="TFs" name="TFs" value="3"/>      <parameter id="kdegX" name="kdegX" value="0.005"/>      <parameter id="kdegA" name="kdegA" value="0.005"/>      <parameter id="R" name="R" value="1" constant="false"/>      <parameter id="C" name="C" value="0" constant="false"/>      <parameter id="I" name="I" value="1" constant="false"/>      <parameter id="X" name="X" value="0" constant="false"/>      <parameter id="A" name="A" value="0" constant="false"/>    </listOfParameters>    <listOfRules>      <rateRule variable="R">        <math xmlns="http://www.w3.org/1998/Math/MathML">          <apply>            <plus/>            <apply>              <minus/>              <apply>                <minus/>                <ci> BR </ci>                <apply>                  <times/>                  <ci> kdegR </ci>                  <ci> R </ci>                </apply>              </apply>              <apply>                <times/>                <ci> kon </ci>                <ci> L </ci>                <ci> R </ci>              </apply>            </apply>            <apply>              <times/>              <ci> koff </ci>              <ci> C </ci>            </apply>            <apply>              <divide/>              <apply>                <times/>                <ci> Rs </ci>                <ci> A </ci>              </apply>              <apply>                <plus/>                <ci> KD </ci>                <ci> A </ci>              </apply>            </apply>          </apply>        </math>      </rateRule>      <rateRule variable="C">        <math xmlns="http://www.w3.org/1998/Math/MathML">          <apply>            <plus/>            <apply>              <minus/>              <apply>                <minus/>                <apply>                  <minus/>                  <apply>                    <times/>                    <ci> kon </ci>                    <ci> L </ci>                    <ci> R </ci>                  </apply>                  <apply>                    <times/>                    <ci> koff </ci>                    <ci> C </ci>                  </apply>                </apply>                <apply>                  <times/>                  <ci> kdegC </ci>                  <ci> C </ci>                </apply>              </apply>              <apply>                <times/>                <ci> k1 </ci>                <ci> C </ci>                <ci> I </ci>              </apply>            </apply>            <apply>              <times/>              <apply>                <plus/>                <ci> k2 </ci>                <ci> k3 </ci>              </apply>              <ci> X </ci>            </apply>          </apply>        </math>      </rateRule>      <rateRule variable="I">        <math xmlns="http://www.w3.org/1998/Math/MathML">          <apply>            <plus/>            <apply>              <minus/>              <apply>                <minus/>                <ci> BI </ci>                <apply>                  <times/>                  <ci> kdegI </ci>                  <ci> I </ci>                </apply>              </apply>              <apply>                <times/>                <ci> k1 </ci>                <ci> C </ci>                <ci> I </ci>              </apply>            </apply>            <apply>              <times/>              <ci> k2 </ci>              <ci> X </ci>            </apply>            <apply>              <divide/>              <apply>                <times/>                <ci> TFs </ci>                <ci> A </ci>              </apply>              <apply>                <plus/>                <ci> KD </ci>                <ci> A </ci>              </apply>            </apply>          </apply>        </math>      </rateRule>      <rateRule variable="X">        <math xmlns="http://www.w3.org/1998/Math/MathML">          <apply>            <minus/>            <apply>              <minus/>              <apply>                <times/>                <ci> k1 </ci>                <ci> C </ci>                <ci> I </ci>              </apply>              <apply>                <times/>                <apply>                  <plus/>                  <ci> k2 </ci>                  <ci> k3 </ci>                </apply>                <ci> X </ci>              </apply>            </apply>            <apply>              <times/>              <ci> kdegX </ci>              <ci> X </ci>            </apply>          </apply>        </math>      </rateRule>      <rateRule variable="A">        <math xmlns="http://www.w3.org/1998/Math/MathML">          <apply>            <minus/>            <apply>              <times/>              <ci> k3 </ci>              <ci> X </ci>            </apply>            <apply>              <times/>              <ci> kdegA </ci>              <ci> A </ci>            </apply>          </apply>        </math>      </rateRule>    </listOfRules>  </model></sbml>