<species id="pSok2" name="pSok2" compartment="V" initialConcentration="1.00" charge="0"/>
<species id="Ime1" name="Ime1" compartment="V" initialConcentration="0.00" charge="0"/>
<species id="pIme1" name="pIme1" compartment="V" initialConcentration="0.00" charge="0"/>
<species id="Ime2" name="Ime2" compartment="V" initialConcentration="0.00" charge="0"/>
</listOfSpecies>

<listOfParameters>
<parameter id="sime1" value="10"/>
<parameter id="sime2" value="10.00"/>
<parameter id="sprimeime2" value="3"/>
<parameter id="dime1" value="1"/>
<parameter id="dprimeime1" value="1"/>
<parameter id="dpime1" value="1"/>
<parameter id="dime2" value="8.00"/>
<parameter id="prim11" value="0.01"/>
<parameter id="urim11" value="0.10"/>
<parameter id="pume6" value="0.30"/>
<parameter id="uume6" value="0.01"/>
<parameter id="psok2" value="0.70"/>
<parameter id="usok2" value="1.00"/>
<parameter id="pime1" value="2"/>
<parameter id="csok2" value="0.05"/>
<parameter id="cime1" value="0.01"/>
<parameter id="c1" value="0.01"/>
