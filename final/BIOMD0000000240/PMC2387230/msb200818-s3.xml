<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
<model name="DegUmodel">
<listOfUnitDefinitions>
      <unitDefinition id="volume">
        <listOfUnits>
          <unit kind="litre" scale="-15" multiplier="1" offset="0"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="substance">
        <listOfUnits>
          <unit kind="item" multiplier="1" offset="0"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
<listOfCompartments>
<compartment name="univ" volume="1.0"/>
</listOfCompartments>
<listOfSpecies>
<species boundaryCondition="false" compartment="univ" initialAmount="0.0" name="AprE"/>
<species boundaryCondition="false" compartment="univ" initialAmount="10.0" name="DegU"/>
<species boundaryCondition="false" compartment="univ" initialAmount="0.0" name="DegUP"/>
<species boundaryCondition="false" compartment="univ" initialAmount="0.0" name="Dim"/>
<species boundaryCondition="false" compartment="univ" initialAmount="0.0" name="mAprE"/>
<species boundaryCondition="false" compartment="univ" initialAmount="0.0" name="mDegU"/>
</listOfSpecies>
<listOfParameters>
<parameter name="Imax" value="0.048"/>
<parameter name="Io" value="0.0040"/>
<parameter name="Irmax" value="0.4"/>
<parameter name="Iro" value="0.02"/>
<parameter name="K" value="7.0"/>
<parameter name="Kdim" value="12.0"/>
<parameter name="Kr" value="7.0"/>
<parameter name="Kr1" value="7.0"/>
<parameter name="R" value="7.0"/>
<parameter name="V" value="1.0"/>
<parameter name="ka" value="0.025"/>
<parameter name="kd" value="0.1"/>
<parameter name="kdeg" value="4.0E-4"/>
<parameter name="kdegA" value="1.0E-4"/>
<parameter name="kdegm" value="0.01"/>
<parameter name="kdephos" value="0.15"/>
<parameter name="kphos" value="0.0040"/>
<parameter name="ksyn" value="0.04"/>
<parameter name="ksyn1" value="0.04"/>
<parameter name="p" value="1.0"/>
<parameter name="q" value="4.0"/>
</listOfParameters>
<listOfRules/>
<listOfReactions>
<reaction name="AprEdeg" reversible="false">
<listOfReactants>
<speciesReference species="AprE" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdeg*AprE">
</kineticLaw>
</reaction>
<reaction name="AprEsyn" reversible="false">
<listOfReactants/>
<listOfProducts>
<speciesReference species="AprE" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="ksyn*(mAprE * univ)"/>
</reaction>
<reaction name="DimerAss" reversible="false">
<listOfReactants>
<speciesReference species="DegUP" stoichiometry="2"/>
</listOfReactants>
<listOfProducts>
<speciesReference species="Dim" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="ka*DegUP^2">
</kineticLaw>
</reaction>
<reaction name="DimerDis" reversible="false">
<listOfReactants>
<speciesReference species="Dim" stoichiometry="1"/>
</listOfReactants>
<listOfProducts>
<speciesReference species="DegUP" stoichiometry="2"/>
</listOfProducts>
<kineticLaw formula="kd*Dim">
</kineticLaw>
</reaction>
<reaction name="degradation1" reversible="false">
<listOfReactants>
<speciesReference species="DegU" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdeg*(DegU * univ)"/>
</reaction>
<reaction name="degradation2" reversible="false">
<listOfReactants>
<speciesReference species="DegUP" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdeg*(DegUP * univ)"/>
</reaction>
<reaction name="degradation3" reversible="false">
<listOfReactants>
<speciesReference species="Dim" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdeg*(Dim * univ)"/>
</reaction>
<reaction name="degradationmRNA" reversible="false">
<listOfReactants>
<speciesReference species="mDegU" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdegm*mDegU">
</kineticLaw>
</reaction>
<reaction name="dephosphorylation" reversible="false">
<listOfReactants>
<speciesReference species="DegUP" stoichiometry="1"/>
</listOfReactants>
<listOfProducts>
<speciesReference species="DegU" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="kdephos*DegUP">
</kineticLaw>
</reaction>
<reaction name="mRNAAprEdeg" reversible="false">
<listOfReactants>
<speciesReference species="mAprE" stoichiometry="1"/>
</listOfReactants>
<listOfProducts/>
<kineticLaw formula="kdegm*mAprE">
</kineticLaw>
</reaction>
<reaction name="mRNAAprEsyn" reversible="false">
<listOfReactants/>
<listOfProducts>
<speciesReference species="mAprE" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="(Kr1/(R+Kr1))*(((Iro*(((Dim * univ)/Kdim)+1.0))/(((1.0+((Dim * univ)/Kdim))+(((Dim * univ)^2.0)/(Kdim^2.0)))+(R/Kr)))+((Irmax*((Dim * univ)^2.0))/((Kdim^2.0)*(((1.0+((Dim * univ)/Kdim))+(((Dim * univ)^2.0)/(Kdim^2.0)))+(R/Kr)))))"/>
</reaction>
<reaction name="phosphorylation" reversible="false">
<listOfReactants>
<speciesReference species="DegU" stoichiometry="1"/>
</listOfReactants>
<listOfProducts>
<speciesReference species="DegUP" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="kphos*DegU">
</kineticLaw>
</reaction>
<reaction name="synthesisDegU" reversible="false">
<listOfReactants/>
<listOfProducts>
<speciesReference species="DegU" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="ksyn1*(mDegU * univ)"/>
</reaction>
<reaction name="synthesismRNA" reversible="false">
<listOfReactants/>
<listOfProducts>
<speciesReference species="mDegU" stoichiometry="1"/>
</listOfProducts>
<kineticLaw formula="((Io*K)/((Dim * univ)+K))+((Imax*(Dim * univ))/((Dim * univ)+K))"/>
</reaction>
</listOfReactions>
</model>
</sbml>
