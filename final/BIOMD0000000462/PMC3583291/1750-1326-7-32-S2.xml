<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model id="AbetaAggregation">
    <listOfUnitDefinitions>
      <unitDefinition id="substance">
        <listOfUnits>
          <unit kind="item"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="cell" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="Abeta" name="AbetaMonomer" compartment="cell" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="AbDim" name="AbetaDimer" compartment="cell" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="AbP" name="AbetaPlaque" compartment="cell" initialAmount="0" hasOnlySubstanceUnits="true"/>
      <species id="Source" compartment="cell" initialAmount="1" hasOnlySubstanceUnits="true" boundaryCondition="true" constant="true"/>
      <species id="Sink" compartment="cell" initialAmount="1" hasOnlySubstanceUnits="true" boundaryCondition="true" constant="true"/>
      <species id="Nep" name="Neprilysin" compartment="cell" initialAmount="1000" hasOnlySubstanceUnits="true"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="kprod" value="1.86e-05"/>
      <parameter id="kdeg" value="2.1e-05"/>
      <parameter id="kdimer" value="1.1783e-07"/>
      <parameter id="kdedimer" value="8.4655e-06"/>
      <parameter id="kdisagg" value="5.4357e-05"/>
      <parameter id="kpf" value="2.785e-06"/>
      <parameter id="kpg" value="0.00574"/>
      <parameter id="kpghalf" value="4"/>
      <parameter id="kdegNep" value="1.8e-10"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="Abetaproduction" reversible="false">
        <listOfReactants>
          <speciesReference species="Source"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abeta"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kprod </ci>
              <ci> Source </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaDegradation" reversible="false">
        <listOfReactants>
          <speciesReference species="Abeta"/>
          <speciesReference species="Nep"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Sink"/>
          <speciesReference species="Nep"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kdeg </ci>
              <ci> Abeta </ci>
              <ci> Nep </ci>
              <cn> 0.001 </cn>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaDimerisation" reversible="false">
        <listOfReactants>
          <speciesReference species="Abeta" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="AbDim"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kdimer </ci>
              <ci> Abeta </ci>
              <apply>
                <minus/>
                <ci> Abeta </ci>
                <cn type="integer"> 1 </cn>
              </apply>
              <cn> 0.5 </cn>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaDedimerisation" reversible="false">
        <listOfReactants>
          <speciesReference species="AbDim"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abeta" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kdedimer </ci>
              <ci> AbDim </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaPlaqueFormation" reversible="false">
        <listOfReactants>
          <speciesReference species="AbDim" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="AbP" stoichiometry="4"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kpf </ci>
              <ci> AbDim </ci>
              <apply>
                <minus/>
                <ci> AbDim </ci>
                <cn type="integer"> 1 </cn>
              </apply>
              <cn> 0.5 </cn>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaPlaqueGrowth" reversible="false">
        <listOfReactants>
          <speciesReference species="Abeta"/>
          <speciesReference species="AbP"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="AbP" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> kpg </ci>
                <ci> Abeta </ci>
                <apply>
                  <power/>
                  <ci> AbP </ci>
                  <cn type="integer"> 2 </cn>
                </apply>
              </apply>
              <apply>
                <plus/>
                <apply>
                  <power/>
                  <ci> kpghalf </ci>
                  <cn type="integer"> 2 </cn>
                </apply>
                <apply>
                  <power/>
                  <ci> AbP </ci>
                  <cn type="integer"> 2 </cn>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="AbetaDisaggregation" reversible="false">
        <listOfReactants>
          <speciesReference species="AbP"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abeta"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kdisagg </ci>
              <ci> AbP </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="NepDegradation" reversible="false">
        <listOfReactants>
          <speciesReference species="Nep"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Sink"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kdegNep </ci>
              <ci> Nep </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
